# Third-party python packages.

# Python targets must have a srcs property, so export an empty file that
# can be used, e.g.
#
#   py_library(
#        name = "foo",
#        srcs = ["//third_party/py:empty.py"],
#        deps = [
#            requirement("foo"),
#        ],
#    )
#
exports_files(["empty.py"])
